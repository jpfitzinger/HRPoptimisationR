@misc{vesperoni_spillovers_2014,
  title = {Spillovers from Unwinding Monetary Support in Advanced Economies},
  abstract = {As prospects in key advanced economies improve, financial conditions will tighten. The effect of outward spillovers from source countries to emerging markets will depend on whether financial conditions are driven by stronger growth (real shocks) or unexpected tightening in financial conditions (money shocks) \textendash{} ~including those due to financial stability concerns or market uncertainty about the exit path. From a recipient's perspective, spillovers will also differ across countries \textendash{} reflecting interactions between domestic fundamentals and policies with the external shock.},
  timestamp = {2017-05-06T10:51:02Z},
  urldate = {2017-05-06},
