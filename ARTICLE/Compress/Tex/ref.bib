@misc{aizenman_monetary_2015,
  title = {Monetary Policy Spillovers and the Trilemma in the New Normal: {{Periphery}} Country Sensitivity to Core Country Conditions},
  shorttitle = {Monetary Policy Spillovers and the Trilemma in the New Normal},
  abstract = {Monetary policies of financial centre countries could have large spillover effects on smaller economies. This column argues that the strength of the links with the centre economies has been the major factor affecting financial conditions in emerging and developing countries. Open macro policies such as the exchange rate regime and financial openness are also important. An economy that pursues greater exchange rate stability and financial openness has a stronger link with the centre economies.},
  timestamp = {2017-05-06T10:50:22Z},
